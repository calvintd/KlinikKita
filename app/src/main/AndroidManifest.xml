<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.calvintd.klinikkita">

    <application
        android:allowBackup="true"
        android:icon="@drawable/klinik"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.beranda.BerandaKlinikActivity"></activity>
        <activity android:name=".activity.beranda.BerandaPasienActivity" />
        <activity android:name=".activity.pendaftaran.DaftarKlinikActivity" />
        <activity android:name=".activity.pendaftaran.DaftarPasienActivity" />
        <activity android:name=".activity.login.LoginKlinikActivity" />
        <activity android:name=".activity.login.LoginPasienActivity" />
        <activity android:name=".activity.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>